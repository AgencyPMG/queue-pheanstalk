{
    "name": "pmg/queue-pheanstalk",
    "description": "A pmg/queue driver implementation for Pheanstalk and Beanstalkd",
    "license": "Apache-2.0",
    "authors": [
        { "name": "Christopher Davis", "email": "chris@pmg.com" }
    ],
    "require": {
        "php": "^8.3",
        "pmg/queue": "^6.2",
        "pda/pheanstalk": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6.23",
        "symfony/phpunit-bridge": "^v7.4.0"
    },
    "autoload": {
        "psr-4": {
            "PMG\\Queue\\Driver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PMG\\Queue\\Driver\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev",
            "dev-version-1": "1.0-dev"
        }
    }
}
